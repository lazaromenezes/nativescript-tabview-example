    <StackLayout loaded="tabOneLoaded">
      <ListView items="{{ items }}">
        <ListView.itemTemplate>
          <Label text="{{ title }}" textWrap="true" />
        </ListView.itemTemplate>
      </ListView>
    </StackLayout>
  </TabViewItem.view>
</TabViewItem>
